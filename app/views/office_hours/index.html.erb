<h1>Listing office_hours</h1>

<table>
  <tr>
    <th>Day</th>
    <th>Start</th>
    <th>End</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @office_hours.each do |office_hour| %>
  <tr>
    <td><%= office_hour.day %></td>
    <td><%= office_hour.start %></td>
    <td><%= office_hour.end %></td>
    <td><%= link_to 'Show', office_hour %></td>
    <td><%= link_to 'Edit', edit_office_hour_path(office_hour) %></td>
    <td><%= link_to 'Destroy', office_hour, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

